{
  "version": 2,
  "builds": [
    { "src": "backend/**/*.js", "use": "@vercel/node" },
    { "src": "frontend/index.html", "use": "@vercel/static" }
  ],
  "routes": [{ "src": "/(.*)", "dest": "/frontend/index.html" }],
  "rewrites": [{ "source": "/(.*)", "destination": "/frontend/index.html" }]
}
