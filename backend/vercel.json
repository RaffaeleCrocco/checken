{
  "version": 2,
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/", "dest": "/" },
    { "src": "/dashboard/*", "dest": "/" },
    { "src": "/checks/details/*", "dest": "/" },
    { "src": "/checks/edit/*", "dest": "/" },
    { "src": "/checks/delete/*", "dest": "/" },
    { "src": "/checks/create", "dest": "/" }
  ]
}
